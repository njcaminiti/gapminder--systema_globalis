        \PY{n+nb}{print}\PY{p}{(}\PY{l+s+s2}{\PYZdq{}}\PY{l+s+s2}{index names are:}\PY{l+s+s2}{\PYZdq{}}\PY{p}{,} \PY{n}{master}\PY{o}{.}\PY{n}{index}\PY{o}{.}\PY{n}{names}\PY{p}{)}
